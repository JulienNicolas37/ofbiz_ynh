    "arguments": {
        "install" : [
            {
                "name": "domain",
                "ask": {
                    "en": "Choose a domain for Apache-OFBiz"
                    "fr": "choisissez un domaine pour Apache-OFBiz"
                },
                "example": "domain.org"
            },
            {
                "name": "path",

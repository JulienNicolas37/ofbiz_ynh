{
    "name": "OFBiz",
    "id": "ofbiz",
    "description": {
        "en": "Apache-OFBiz ERP"
        "fr": "L'ERP de gestion d'entreprise Apache-OFBiz"
    },
    "developer": {
        "name": "Julien NICOLAS",
        "email": "julien.nicolas@nomaka.fr",
        "url": "http://nomaka.fr"
    },
